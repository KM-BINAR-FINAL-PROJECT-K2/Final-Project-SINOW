{
  "name": "final-project-fix",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config bin/www",
    "dev": "nodemon -r dotenv/config bin/www",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
    "db:create:test": "sequelize db:create --env test",
    "db:drop:test": "sequelize db:drop --env test",
    "db:migrate:test": "sequelize db:migrate --env test",
    "db:migrate:undo:test": "sequelize db:migrate:undo:all --env test",
    "db:seed:test": "sequelize db:seed:undo:all --env test && sequelize db:seed:all --env test",
    "lint": "eslint .",
    "fixlint": "eslint . --fix",
    "test": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000",
    "test:auth": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000 --runTestsByPath test/Auth.api.spec.js",
    "test:course": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000 --runTestsByPath test/Course.api.spec.js",
    "test:user": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000 --runTestsByPath test/User.api.spec.js",
    "test:module": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000 --runTestsByPath test/Module.api.spec.js",
    "test:notification": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000 --runTestsByPath test/Notification.api.spec.js",
    "test:chapter": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000 --runTestsByPath test/Chapter.api.spec.js",
    "test:benefit": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000 --runTestsByPath test/Benefit.api.spec.js",
    "test:category": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit --testTimeout=20000 --runTestsByPath test/Category.api.spec.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.0",
    "imagekit": "^3.2.5",
    "jsonwebtoken": "^9.0.2",
    "midtrans-client": "^1.3.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "randomstring": "^1.3.0",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "swagger-ui-express": "^5.0.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "supertest": "^6.3.1"
  }
